<h1>Lista de Usuarios</h1>

<table>
    <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Correo</th>
    </tr>
    {{#each users }}
    <tr>
        <td>{{this.firstName}}</td>
        <td>{{this.lastName}}</td>
        <td>{{this.email}}</td>
    </tr>
    {{/each}}
</table>
